{
  "compilerOptions": {
    "target": "es2017",
    "lib": ["dom", "dom.iterable", "esnext", "es2017", "es2015"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "strictBindCallApply": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "experimentalDecorators": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "react",
    "incremental": true,


    "typeRoots": ["./node_modules/@types"],

    "plugins": [
      {
        "name": "next"
      }
    ],
    "baseUrl": ".",
      "paths": {
        "@/*": ["./src/*"],
        "@/app/shared/shared_error_handling": ["./src/app/configs/declarations/global.d.ts"],
        "@/pages/*": ["./src/app/pages/*"],
        "@/drawingLibrary": ["./src/app/configs/declarations/global.d.ts"],
        "@/api/*": ["./src/app/api/*"],
        "@/backendStructure/*": ["./src/app/configs/appStructure/*"],
        "@/frontendStructure/*": ["./src/app/configs/appStructure/*"],
        "@/calendar/*": ["./src/app/components/calendar/*"],
        "@/documents/*": ["./src/app/components/documents/*"],
        "@/projects/*": ["./src/app/components/projects/*"],
        "@/tasks/*": ["./src/app/components/tasks/*"],
        "@/support/*": ["./src/app/components/support/*"],
        "@/animations/*": ["./src/app/components/animations/*"],
        "@/stores/*": ["./src/app/components/state/stores/*"],
        "@/videos/*": ["./src/app/components/video/*"],
        "@/hooks/*": ["./src/app/components/hooks/*"],
        "@/users/*": ["./src/app/components/users/*"],
        "@/versions/*": ["./src/app/components/versions/*"],
        "@/configs/*": ["./src/app/configs/*"],
        "@/snapshots/*": ["./src/app/components/snapshots/*"],
        "@/configs/appStructure/components/*": ["./src/app/configs/appStructure/components/*"],
        "@/configs/appStructure/pages/*": ["./src/app/configs/appStructure/pages/*"],
        "@/configs/appStructure/pages/onboarding/*": ["./src/app/configs/appStructure/pages/onboarding/*"],
        "@/configs/appStructure/pages/dashboards/*": ["./src/app/configs/appStructure/pages/dashboards/*"],
        "@/configs/appStructure/pages/forms/*": ["./src/app/configs/appStructure/pages/forms/*"],
        "@/configs/appStructure/pages/layouts/*": ["./src/app/configs/appStructure/pages/layouts/*"],
        "@/components/support/NotificationContext": ["./src/app/components/support/NotificationContext.tsx"],
        "@/management/*": ["./src/app/components/management/*"],
        "@/context/*": ["./src/app/context/*"],
        "@/onboarding/*": ["./src/app/pages/onboarding/*"],
        "@/dashboards/*": ["./src/app/pages/dashboards/*"],
        "@/forms/*": ["./src/app/pages/forms/*"],
        "@/layouts/*": ["./src/app/pages/layouts/*"],
        "@/searches/*": ["./src/app/pages/searches/*"],
        "@/tracker/*": ["./src/app/components/tracker/*"],
        "@/libraries/*": ["./src/app/components/libraries/*"],
        "@/menu/*": ["./src/app/components/libraries/menu/*"]
      }
  },
 "include": [
  "next-env.d.ts",
  "**/*.d.ts",
  "**/*.ts",
  "**/*.tsx",
  ".next/types/**/*.ts",

  "src/global.d.ts",
  "src/shared-error-handling.d.ts",
  "src/drawing-library.d.ts",
  "src/presentations-library.d.ts",
  "src/excel4node.d.ts",
  "src/react-speech-recognition.d.ts",

  "src/app/screenFunctionality/shortcut_keys.js",
  "subscriptions/Subscription.tsx",
  "src/app/components/calendar/CalendarEvent.jsx",
  "src/app/components/calendar/CalendarComponent.jsx",
  "src/app/components/calendar/YourCalendarLibrary.js",
  "src/app/components/projects/Project.jsx",
  "src/app/components/web3/dAppAdapter/remove",
  "src/app/components/state/mobx/reactions.tsx",
  "src/app/components/phases/collaborationPhase/CollaborationContext.js",
  "src/app/documents/**/*",
  "src/app/utils/EmojiPickerUtils.jsx",
  "src/app/components/libraries/ui/UILibrary.tsx",
  "src/app/pages/dashboards/DashboardOverview.jsx",
  "src/app/shared/*.d.ts",
  "src/app/shared/drawingLibrary.d.ts",
  "src/app/components/documents/types.d.ts",
  "src/app/components/phases/PhasesNavigation.jsx",
  "src/app/utils/MyPromise.tsx",
  "src/app/components/communications/chat/features/clearChatImageCache.js",
  "src/app/pages/forms/DynamicConfigurationLogic.jsx",
  "src/app/pages/content/CaptionManagementPage.js",
  "src/app/configs/declarations/**/*.d.ts",
  "src/app/components/libraries/toolbar/Sidebar.tsx",
  "src/app/context/DashboardContext.tsx",
  "src/app/components/calendar/CalendarWeek.tsx",
  "src/app/components/models/realtime/RealTimeDataCollection.tsx"
],
  "exclude": [
    "node_modules",
    "babel.config.js",
    "metro.config.js",
    "jest.config.js"
  ]
}